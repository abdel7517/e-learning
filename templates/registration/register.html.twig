




<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width,initial-scale=1.0">

        <title>{% block title %}E-learnia - {{ 'Register' | trans }}{% endblock %}</title>
        <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css">
        {% block stylesheets %}        
		<link rel="stylesheet" href="{{ asset('assets/css/admin.css') }}">
		{% endblock %}
        <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css"
              integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
        <!-- CHIVO FONT -->
        <link href="https://fonts.googleapis.com/css?family=Chivo:300,400,700,900&display=swap" rel="stylesheet">
        <!-- STYLESHEET -->
        <link rel="stylesheet" href="{{ asset('assets/css/app.css') }}">

        <link rel='shortcut icon' type='image/x-icon' href='{{ asset('assets/img/favicon.ico') }}' />
    </head>
    <body class="loginbody">
	{% block nav %}
	<nav
				class="navbar navbar-expand-md navbar-dark bg nav-bar-color">
				<!-- LOGO   -->

				<a class="navbar-brand" href="{{ path('homepage') }}"><img src="{{ asset('assets/img/lightlogo-15.png') }}" height="50px" alt="logo"></a>

				<!-- HAMBURGER ICON BUTTON   -->
				<button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarTogglerDemo02" aria-controls="navbarTogglerDemo02" aria-expanded="false" aria-label="Toggle navigation">
					<span class="navbar-toggler-icon"></span>
				</button>

				<div class="collapse navbar-collapse nav-bar-collapse" id="navbarTogglerDemo02">

					{% set searchbar = searchbar.getSearchbar() %}
					{{ form_start(searchbar, {'attr': {'class': 'form-inline my-2 my-lg-0'}}) }}
					{{ form_widget(searchbar.keywords, {'attr': {'class': 'form-control mr-sm-2', 'placeholder': 'Search'}}) }}
				</form>

				<!-- LINKS   -->

				<ul class="navbar-nav ml-auto mt-2 mt-lg-0">

				<li class="nav-item formnav">
					</li
					<li class="nav-item active hubnav">
						<a class="nav-link" data-toggle="tooltip" data-placement="right" title="{{ 'Portal' | trans }}" href="{{ path('portal') }}"><img height="25px" src="{{ asset('assets/img/lightlogo-09.png') }}" alt="{{ 'Portal' | trans }}"><span class="sr-only">(current)</span>
						</a>
					</li>


					<li class="nav-item formnav">
						<a class="nav-link" data-toggle="tooltip" data-placement="right" title="{{ 'Meetingplace' | trans }}" href="{{ path('forum') }}"><img height="25px" src="{{ asset('assets/img/lightlogo-10.png') }}" alt="{{ 'Meetingplace' | trans }}"></a>
					</li>

					<li class="nav-item profilenav">
						<div class="nav-link" id="profiledrop"><img data-toggle="tooltip" data-placement="right" title="{{ 'Profile' | trans }}" height="25px" src="{{ asset('assets/img/lightlogo-11.png') }}" alt="{{ 'Profile' | trans }}"></div>
						<div id="reveal" class="gone">
							<a class="nav-link" href="{{ path('profile') }}">{{ 'My profile' | trans }}</a>
							<a class="nav-link" href="{{ path('app_logout') }}">{{ 'Logout' | trans }}</a>
						</div>
					</li>
					<li id="timer">
						
					</li>
					{# <li class="nav-item dropdown">
						{{ include('language_switcher/_switcher.html.twig') }}
					</li> #}
				</ul>

			</div>
		</nav>

	{% endblock %}	
        <div class="titlewrap">
    {% block back %}
        <h2>{{ 'Ajoutez un nouvelle utilisateur' | trans}}</h2>
    </div>
    {% endblock %}

    {% for type, messages in app.session.flashbag.all() %}
        {% for message in messages %}
            {% if type == 'error' %}{% set type = 'danger' %}{% endif %}
            <div class="alert alert-{{type}}" role="alert">
                {{ message }}
            </div>
        {% endfor %}
    {% endfor %}

{% block body %}
<div class="sidebar">

    {{ form_start(registrationForm) }}
{#        {{ form_row(registrationForm.avatar) }}#}
        <div class="loginwrap">
                {{ form_row(registrationForm.name) }}
                {{ form_row(registrationForm.email) }}
                {{ form_row(registrationForm.username) }}
                {{ form_row(registrationForm.plainPassword) }}
                {{ form_row(registrationForm.passwordRepeat) }}
                {{ form_row(registrationForm.agreeTerms) }}

                <button class="button button_1">{{ 'Ajoutez' | trans }}</button>
        </div>
    {{ form_end(registrationForm) }}
        <p class="accountgot">
                <a href="{{ path('partner') }}">{{ 'Retorunez au tableau admin' | trans }}</a>
        </p>
</div>
{% endblock %}    <footer><a href="https://becode.org" target="_blank">Â© BeCode.org</a></footer>
        {% block javascripts %}{% endblock %}
